      <soap:operation style="document"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="FeedbackOrderService">
    <wsdl:port name="RatePersonPort" binding="ps:FeedbackSOAPBinding">
      <soap:address location="http://localhost:8080/SOAProject/services/FeedbackOrderService"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
